<?xml version="1.0"?><tsung loglevel="notice" version="1.0">

  <clients>
    <client host="localhost" use_controller_vm="true" maxusers="15000"/>
  </clients>

  <servers>
    <server host="54.174.90.113" port="80" type="tcp"/>
  </servers>


  <load>
    <arrivalphase phase="1" duration="60" unit="second">
      <users arrivalrate="1" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="2" duration="60" unit="second">
      <users arrivalrate="2" unit="second"/>
    </arrivalphase>
</load>

  <options>
    <option name="global_ack_timeout" value="2000"/>

    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>

  <sessions>
    <session name="User login and buy product" probability="80" type="ts_http">
      <request>
        <http url="/" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <setdynvars sourcetype="random_number" start="1" end="500">
        <var name="username"/>
      </setdynvars>

      <request>
        <http url="/buyers/sign_in" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <request subst="true">
        <http url="/buyers/sign_in" version="1.1" method="POST" contents="utf8=%E2%9C%93&amp;buyer%5Bemail%5D=%%_username%%%40a.com&amp;buyer%5Bpassword%5D=12345678&amp;buyer%5Bremember_me%5D=0&amp;commit=Log+in">
        </http>
      </request>

      <thinktime value="2" random="true"/>

      <setdynvars sourcetype="random_number" start="1" end="100">
        <var name="random_product"/>
      </setdynvars>

      <setdynvars sourcetype="random_number" start="100" end="1000">
        <var name="random_price"/>
      </setdynvars>

      <request subst="true">
        <http url="/line_items?price=%%_random_price%%&amp;product_id=%%_random_product%%&amp;quantity=1" version="1.1" method="POST" contents="price=%%_random_price%%&amp;product_id=%%_random_product%%&amp;quantity=1&amp;commit=Add+to+Cart">
        </http>
      </request>

      <thinktime value="2" random="true"/>

      <request>
        <http url="/orders/new" version="1.1" method="GET"/>
      </request>

      <thinktime value="3" random="true"/>

      <request subst="true">
        <http url="/orders" version="1.1" method="POST" contents="order%5Bname%5D=Joe+Gaucho&amp;order%5Baddress%5D=Santa+Barbara+CA+93106&amp;order%5Bemail%5D=a%40a.com&amp;order%5Bpay_type%5D=Check&amp;commit=Place+Order">
        </http>
      </request>

      </session>

    <session name="Seller edit products" probability="20" type="ts_http">
      <request>
        <http url="/" version="1.1" method="GET"/>
      </request>

      <setdynvars sourcetype="random_number" start="1" end="500">
        <var name="username"/>
      </setdynvars>

      <request>
        <http url="/sellers/sign_in" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <request subst="true">
        <http url="/sellers/sign_in" version="1.1" method="POST" contents="utf8=%E2%9C%93&amp;seller%5Bemail%5D=%%_username%%%40a.com&amp;seller%5Bpassword%5D=12345678&amp;seller%5Bremember_me%5D=0&amp;commit=Log+in">
        </http>
      </request>

      <thinktime value="2" random="true"/>

      <request>
        <http url="/products" version="1.1" method="GET"/>
      </request>

      <setdynvars sourcetype="random_number" start="1" end="100">
        <var name="random_product"/>
      </setdynvars>

      <request subst="true">
        <http url="/products/%%_random_product%%" version="1.1" method="DELETE" contents="product_id=%%_random_product%%">
        </http>
      </request>

      <request>
          <http url="/products/new" version="1.1" method="GET"/>
      </request>

      <setdynvars sourcetype="random_string" length="5">
          <var name="prod_name"/>
      </setdynvars>
      <setdynvars sourcetype="random_string" length="15">
          <var name="prod_desc"/>
      </setdynvars>
      <setdynvars sourcetype="random_number" start="100" end="1000">
        <var name="price"/>
      </setdynvars>
      <setdynvars sourcetype="random_number" start="1" end="100">
        <var name="quantity"/>
      </setdynvars>

      <request subst="true">
        <http url="/products" version="1.1" method="POST" contents="utf8=%E2%9C%93&amp;product%5Btitle%5D=%%_prod_name%%&amp;product%5Bdescription%5D=%%_prod_desc%%&amp;product%5Bimage_url%5D=a.jpg&amp;product%5Bprice%5D=%%_price%%&amp;product%5Bquantity%5D=%%_quantity%%&amp;commit=Create+Product">
        </http>
      </request>

    </session>
  </sessions>
</tsung>
